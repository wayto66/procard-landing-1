    .desktop{display: none !important}
    .main-header .wrapper-topo{justify-content: center;}
    .wrapper-input{width: 100%;}
    .sobre-home .text-head{padding: 0 10px;}
    .sobre-home .new-box .item-box{margin: 20px 18px;}
    .home .beneficios .titulo-head{font-size: 35px;line-height: 40px;}

    .fixed-chat{display: none;}

    .box-flex{display: block!important;}
    .box-banners .banners .item-banner .box-form-banner .box-form .titulo{font-size: 20px;}
    .box-banners .banners .item-banner{height: 100%;max-height: 100%;}
    .box-banners .banners .item-banner .box-form-banner{display: block;text-align: center;}
    .box-banners .banners .item-banner .box-form-banner{position: initial;padding: 0 10px;}
    .box-banners .banners .item-banner .box-form-banner .box-form{margin: 0 auto;}
    .box-banners .banners .item-banner .box-form-banner .box-form .button-form{padding: 10px 24px;}
    .box-banners .banners .item-banner .box-form-banner .box-cont{display: block;text-align: center;padding-top: 40px;padding-bottom: 40px;}
    .box-banners .banners .item-banner .box-form-banner .text-box.new .titulo{font-size: 55px;line-height: 60px;}
    .box-banners .banners .item-banner .box-form-banner .text-box.new{margin: 0 auto 50px;}
    
    .sobre-home .bg-orange .text{padding: 0 10px;text-align: center;}
    
    .box-banners .banners .item-banner{padding: 187px 0px 90px;}
    .home .processos .box-flex{text-align: center;}
    .home .processos .box-flex .item{max-width: 100%;display: block;}
    
    .sobre-home .bg-orange .item-bg{max-width: 100%;}
    .sobre-home .titulo-head{font-size: 35px;}
    .sobre-home .titulo-head-two{font-size: 28px;}
    .home .segmentos .box-itens{display: block;}

    .home .segmentos .item{margin: 30px auto;}

    .sobre-home .bg-orange .item-bg{margin: 16px 0px;text-align: center;}
    .zap-home .box-flex{text-align: center;text-decoration: none;}

    .box-copyright .copyright{display: block;text-align: center;}

    .home .beneficios .box-tree{margin: 70px auto 0px;}
