/* Dispositivos com tela acima de 1170px */
@media(min-width: 1170px){

}

/* Dispositivos com tela acima de 1024px */
@media(min-width: 1024px){
    .mobile{display: none !important}
    .desktop{}

    .col-1,
    .col-1-1{float: left;width: 100%;}
    .col-1-2{float: left;width: 50%;}
    .col-1-3{float: left;width: calc(100%/3);}
    .col-1-4{float: left;width: calc(100%/4);}
    .col-1-5{float: left;width: calc(100%/5);}
    .col-2-5{float: left;width: calc(200%/5);}
    .col-2-3{float: left;width: calc(200%/3);}
    .col-5-12{float: left;width: calc(500%/12);}
    .col-2-12{float: left;width: calc(200%/12);}
    .col-2-14{float: left;width: calc(200%/16);}
    
    .sobre-home .new-box .item-box .titulo{height: 46px;overflow: hidden;}
    .sobre-home .box-itens .item{height: 394px;vertical-align: middle;}
    /*.box-banners .banners .item-banner .box-form-banner .box-form{visibility: hidden;}*/
    
}


/* Dispositivos com tela entre de 1024px e 1169px */
@media(min-width: 1024px) and (max-width: 1169px){

    .sobre-home .text-head{padding: 0 10px;}
    .sobre-home .box-itens .item{max-width: 240px;margin: 10px 6px}
    .main-header .wrapper-topo{padding: 0 10px;}
    .sobre-home .new-box .item-box{margin: 0 11px;}
    .home .segmentos{padding: 30px 10px;}
    .sobre-home .bg-orange .box-flex{padding: 27px 10px 18px 10px;}
    .box-banners .banners .item-banner .box-form-banner .box-cont{padding-left: 10px;padding-right: 10px;}
}


/* Dispositivos com tela abaixo de 1023px */
@media(max-width: 1023px){
