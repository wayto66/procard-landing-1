.fixed-chat .thumb .text-chat::before{content: "";background: url(../images/new-arrow.svg) no-repeat center;position: absolute;width: 18px;height: 18px;right: 21%;top: 13px;transform: rotateZ(180deg);}
.fixed-chat.active .thumb .text-chat::before{content: "";background: none;}
.fixed-chat.active .thumb .text-chat::after{content: "";background: url(../images/new-arrow.svg) no-repeat center!important;position: absolute;width: 18px;height: 18px;right: 21%;top: 13px;}
